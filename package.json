{
  "name": "@superexpert-ai/websearch-tool",
  "version": "1.0.2",
  "description": "Enable web search from chat",
  "keywords": [
    "superexpert-ai",
    "plugin"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Superexpert/superexpert-ai-websearch-tool"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "author": "Stephen Walther",
  "main": "./dist/websearch-tool.cjs",
  "module": "./dist/websearch-tool.js",
  "exports": {
    ".": {
      "types": "./dist/websearch-tool.d.ts",
      "import": "./dist/websearch-tool.js",
      "require": "./dist/websearch-tool.cjs"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.MD"
  ],
  "scripts": {
    "build": "rimraf dist && tsup",
    "prepublishOnly": "npm run build",
    "release:patch": "npm run build && npm version patch -m \"chore(release): %s\" && npm publish",
    "release:minor": "npm run build && npm version minor -m \"feat!: %s\" && npm publish",
    "release:major": "npm run build && npm version major -m \"feat!: %s\" && npm publish",
    "clean": "rimraf dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@superexpert-ai/framework": "^1.0.2",
    "@tavily/core": "^0.3.3",
    "dotenv": "^16.4.7"
  },
  "devDependencies": {
    "@types/node": "^22.13.10",
    "typescript": "^5.8.2",
    "tsup": "^8.4.0"
  }
}
